<p id="notice"><%= notice %></p>

<h1>Composter Updates</h1>

<table>
  <thead>
    <tr>
      <th>Wetweight</th>
      <th>Dryweight</th>
      <th>Cnration</th>
      <th>Composter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @composter_updates.each do |composter_update| %>
      <tr>
        <td><%= composter_update.wetWeight %></td>
        <td><%= composter_update.dryWeight %></td>
        <td><%= composter_update.cNRation %></td>
        <td><%= composter_update.Composter %></td>
        <td><%= link_to 'Show', composter_update %></td>
        <td><%= link_to 'Edit', edit_composter_update_path(composter_update) %></td>
        <td><%= link_to 'Destroy', composter_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Composter Update', new_composter_update_path %>
